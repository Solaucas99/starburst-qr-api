{
  "name": "starburst-qr",
  "version": "1.0.0",
  "main": "app.ts",
  "scripts": {
    "build": "tsc",
    "build:sucrase": "sucrase ./src -d ./dist --transforms typescript,imports",
    "production": "pm2 start dist/server.js --watch --no-daemon",
    "start": "docker-compose up -d",
    "dev": "nodemon src/server.ts",
    "test": "$env:NODE_ENV=test & jest --runInBand --detectOpenHandles",
    "testLink": "$env:NODE_ENV=test & jest ValidateUpdateVisitorLinkUseCase --runInBand --detectOpenHandles",
    "test:silent": "$env:NODE_ENV=test & jest --watchAll --silent --noStackTrace"
  },
  "keywords": [
    "startburst-qr"
  ],
  "author": "Solaucas",
  "license": "ISC",
